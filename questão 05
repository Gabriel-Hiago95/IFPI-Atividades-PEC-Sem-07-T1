def imc(h, p):
    IMC = p / (h ** 2)
    return IMC

def main():
    altura = float(input('Entre com sua altura em m: ').strip())
    peso = float(input('Entre com seu peso em kg: ').strip())
    IMC = imc(altura, peso)
    if IMC < 18.5:
        print('Abaixo do peso')
    elif 18.5 <= IMC < 25:
        print('Peso normal')
    elif 25 <= IMC < 30:
        print('Sobrepeso')
    elif 30 <= IMC < 35:
        print('Obeso Leve')
    elif 35 <= IMC < 40:
        print('Obeso Moderado')
    else:
        print('Obeso MÃ³rbido')

if __name__ == '__main__':
    main()
